{
  "name": "create-boilerplate-vite-react-ts",
  "description": "Tracktor boilerplate vite react ts",
  "private": false,
  "license": "ISC",
  "version": "2.2.0",
  "type": "module",
  "repository": {
    "url": "https://github.com/Tracktor/create-boilerplate-vite-react-ts"
  },
  "bin": {
    "create-boilerplate-vite-react-ts": "bin/create-app.cjs"
  },
  "scripts": {
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc && vite build",
    "create": "node bin/create-app.cjs dist --axios --i18next",
    "lint": "eslint . --max-warnings=0 && tsc --noEmit",
    "preview": "vite preview",
    "test": "yarn lint && vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.26.1",
    "axios": "^1.3.4",
    "i18next": "^22.4.11",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint-config-react-tracktor": "^1.0.5",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  }
}
