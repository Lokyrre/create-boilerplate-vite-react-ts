    "@vitejs/plugin-react": "^2.2.0",
    "eslint-config-react-tracktor": "^1.0.5",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.26.3"
